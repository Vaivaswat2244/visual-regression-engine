{
  "name": "visual-regression-engine",
  "version": "1.0.1",
  "description": "A modular visual comparison engine for Processing and p5.js regression testing",
  "main": "src/index.js",
  "files": [
    "src/",
    "examples/",
    "LICENSE",
    "README.md"
  ],
  "bin":{
    "visusal-regression-engine": "./bin/cli.js"
  },
  "scripts": {
    "build:binaries": "npm run build:win && npm run build:linux && npm run build:macos && npm run build:macos-arm64",
    "build:all-platforms": "npm run build:linux && npm run build:win && npm run build:macos && npm run build:macos-arm64",
    "build:win": "nexe dist/cli.js --build --verbose --target windows-x64-18.17.0 --output binaries/visual-regression-engine-win.exe",
    "build:linux": "nexe dist/cli.js --build --verbose --target linux-x64-18.17.0 --output binaries/visual-regression-engine-linux", 
    "build:macos": "nexe dist/cli.js --build --verbose --target darwin-x64-18.17.0 --output binaries/visual-regression-engine-macos",
    "build:macos-arm64": "nexe dist/cli.js --build --verbose --target darwin-arm64-18.17.0 --output binaries/visual-regression-engine-macos-arm64"
  },
  "keywords": [
    "visual-testing",
    "regression-testing",
    "p5js",
    "processing",
    "image-comparison",
    "creative-coding",
    "testing",
    "automation",
    "canvas",
    "pixelmatch",
    "visual-regression"
  ],
  "author": {
    "name": "Vaivaswat",
    "url": "https://github.com/Vaivaswat2244"
  },
  "license": "MIT",
  "peerDependencies": {
    "p5": "^1.0.0"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "jsdom": "^26.1.0",
    "pixelmatch": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nexe": "^4.0.0-rc.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vaivaswat2244/visual-regression-engine.git"
  },
  "bugs": {
    "url": "https://github.com/Vaivaswat2244/visual-regression-engine/issues"
  },
  "homepage": "https://github.com/Vaivaswat2244/visual-regression-engine#readme"
}
